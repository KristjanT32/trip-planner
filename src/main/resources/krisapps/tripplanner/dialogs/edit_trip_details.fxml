<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox fx:id="rootPane" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minWidth="400.0" xmlns="http://javafx.com/javafx/25" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label text="Edit trip details">
         <font>
            <Font name="Segoe UI Bold" size="20.0" />
         </font>
      </Label>
      <VBox spacing="10.0">
         <children>
            <VBox maxWidth="1.7976931348623157E308">
               <children>
                  <Label text="Trip name">
                     <VBox.margin>
                        <Insets bottom="5.0" />
                     </VBox.margin>
                     <font>
                        <Font name="Segoe UI" size="12.0" />
                     </font>
                  </Label>
                  <TextField fx:id="nameBox" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" promptText="Name">
                     <font>
                        <Font name="Segoe UI Light" size="20.0" />
                     </font>
                  </TextField>
               </children>
            </VBox>
            <VBox maxWidth="1.7976931348623157E308">
               <children>
                  <Label text="Trip destination">
                     <VBox.margin>
                        <Insets bottom="5.0" />
                     </VBox.margin>
                     <font>
                        <Font name="Segoe UI" size="12.0" />
                     </font>
                  </Label>
                  <TextField fx:id="destinationBox" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" promptText="Destination">
                     <font>
                        <Font name="Segoe UI Light" size="20.0" />
                     </font>
                  </TextField>
               </children>
            </VBox>
         </children>
         <VBox.margin>
            <Insets top="10.0" />
         </VBox.margin>
      </VBox>
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
